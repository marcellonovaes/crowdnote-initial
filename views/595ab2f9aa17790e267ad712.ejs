<head>
	<script src='https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
</head>
<body>
<p align=center>Fingerprint:<%= fingerprint %></p>
<p align=center>Item:<%= item %></p>
<p align=center>
	  <video id="video" width="600" controls  preload>
		<source src="data:video/<%= mime %>;base64,<%= v64 %>" type="video/<%= mime %>"> 
 	   	Your browser does not support HTML5 video.
	  </video>
</p>
<form action="http://<%= host %>/jobs/<%= job %>">
  0
  <input type="range"  id="a" name="a" value="50">
  100 +
  <input type="number" id="b" name="b" value="50">
  =
  <output name="x" for="a b"></output>
  <br><br>
  <input type="submit">
</form>


	<script>
		var video = document.getElementById('video');
		video.currentTime = <%= start %>;

		video.addEventListener("timeupdate", function() {

			if(this.currentTime < <%= start %>){
				video.currentTime = <%= start %>;
				video.pause();
			}


			if(this.currentTime > <%= stop %>){
				video.pause();
				video.currentTime = <%= start %>;
			}

		}, false);


	</script>

</body>


